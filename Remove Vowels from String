public class RemoveVowels {
    public static void main(String[] args) {

        String str = "Education";
        String result = "";

        for (int i = 0; i < str.length(); i++) {
            char ch = str.charAt(i);
            if ("AEIOUaeiou".indexOf(ch) == -1)
                result += ch;
        }

        System.out.println(result);
    }
}
