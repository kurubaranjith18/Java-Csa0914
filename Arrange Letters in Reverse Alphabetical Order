import java.util.Arrays;

public class ReverseAlphabetOrder {
    public static void main(String[] args) {

        String str = "PROGRAM";
        char arr[] = str.toCharArray();

        Arrays.sort(arr);

        for (int i = arr.length - 1; i >= 0; i--)
            System.out.print(arr[i]);
    }
}
